/**
 * 16-bit bitwise Or:
 * for i = 0..15 out[i] = (a[i] or b[i])
 */

CHIP Or16 {
    IN a[16], b[16];
    OUT out[16];

    PARTS:
		Not16(in=a, out=w1);
		Not16(in=b, out=w2);
		And16(a=w1, b=w2, out=w3);
		Not16(in=w3, out=out);
}